<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="crm_lead_dna_view_form" model="ir.ui.view">
        <field name="name">crm.lead_dna_view_form</field>
        <field name="model">crm.lead</field>
        <field name="inherit_id" ref="crm.crm_lead_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='lead']/group[1]/group[2]/field[@name='function']" position="after">
                <field name="dna" string="Deal"/>
                <field name="final_user_id"/>
                <field name="channel" string="Canal"/>
            </xpath>
        </field>
    </record>
</odoo>
